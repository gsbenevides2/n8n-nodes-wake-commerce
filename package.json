{
	"name": "@gsbenevides2/n8n-nodes-wake-commerce",
	"version": "1.0.0",
	"description": "n8n community node for Wake Commerce (formerly Tray Commerce) API integration",
	"license": "MIT",
	"homepage": "https://github.com/gsbenevides2/n8n-nodes-wake-commerce",
	"keywords": [
		"n8n-community-node-package",
		"n8n",
		"wake-commerce",
		"tray-commerce",
		"ecommerce",
		"products"
	],
	"author": {
		"name": "Guilherme da Silva Benevides",
		"email": "git@gui.dev.br"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/gsbenevides2/n8n-nodes-wake-commerce.git"
	},
	"publishConfig": {
		"registry": "https://npm.pkg.github.com"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"strict": true,
		"credentials": [
			"dist/credentials/WakeCommerceApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/WakeProducts/WakeProducts.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"eslint": "9.32.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
